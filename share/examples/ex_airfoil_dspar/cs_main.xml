            <layup>lyp_skin_graphite</layup>
            <layup>lyp_spar</layup>
        </segments> -->
        <!-- <segments>
            <baseline>line_spar_top</baseline>
            <layup>lyp_skin_graphite</layup>
            <layup>lyp_spar</layup>
        </segments> -->
        <segments>
            <baseline>line_spar_web_aft</baseline>
            <layup>lyp_skin_graphite</layup>
            <layup>lyp_spar</layup>
        </segments>
        <!-- <segments>
            <baseline>bsl_le</baseline>
            <layup>lyp_steel</layup>
            <layup>lyp_skin_graphite</layup>
            <layup>lyp_le</layup>
        </segments> -->
        <!-- <segments>
            <baseline>bsl_le_top</baseline>
            <layup>lyp_steel</layup>
            <layup>lyp_skin_graphite</layup>
            <layup>lyp_le</layup>
        </segments> -->
        <!-- <segments>
