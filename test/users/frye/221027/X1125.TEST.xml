  <!-- =========================================================== -->
  <!-- Layup Definition -->
  <!-- =========================================================== -->
  <layups>
    <layup name="lyp_spar">
      <layer lamina="T650-35 12k/976_0.0052">-20:1</layer>
      <layer lamina="T650-35 12k/976_0.0052">37:1</layer>
      <layer lamina="T650-35 12k/976_0.0052">-32:1</layer>
      <layer lamina="T650-35 12k/976_0.0052">-45:1</layer>
    </layup>
  </layups>
  <!-- =========================================================== -->
  <!-- Component Definition via Segments (Baseline + Layup) -->
  <!-- =========================================================== -->  
  <component name="spar">
    <!-- <segment>
      <baseline>bsl_spar</baseline>
      <layup direction="right">lyp_spar</layup>
    </segment> -->
    <segment>
      <baseline>bsl_spar_top</baseline>
      <layup direction="right">lyp_spar</layup>
    </segment>
    <segment>
      <baseline>bsl_spar_bottom</baseline>
      <layup direction="right">lyp_spar</layup>
    </segment>
    <segment>
      <baseline>bsl_spar_le</baseline>
      <layup direction="right">lyp_spar</layup>
    </segment>
    <segment>
      <baseline>bsl_spar_te</baseline>
      <layup direction="right">lyp_spar</layup>
    </segment>
  </component>


  <component name="le" depend="spar">
    <segment>
      <baseline>bsl_le</baseline>
      <layup direction="right">lyp_spar</layup>
    </segment>
  </component>


