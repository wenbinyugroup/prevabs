<cross_section name="uh60a_section" format="1">
    <include>
        <material>material_database_us_ft</material>
    </include>
    <analysis>
        <model>1</model>
    </analysis>
    <general>
        <scale>1.73</scale>
        <mesh_size>0.002</mesh_size>
        <element_type>linear</element_type>
    </general>



    <baselines>

        <line name="ln_af" type="airfoil">
            <points data="file" format="1" direction="-1" header="0">sc1095m1.dat</points>
            <flip>1</flip>
            <!-- <reverse>1</reverse> -->
        </line>

        <point name="p1" on="ln_af" by="x2" which="top">0.7316642415</point>
        <point name="p2" on="ln_af" by="x2" which="bottom">0.7316642415</point>
        <point name="p3" on="ln_af" by="x2" which="top">0.3850776842</point>
        <point name="p4" on="ln_af" by="x2" which="bottom">0.3850776842</point>

        <point name="p5" on="ln_af" by="x2" which="top">0.98</point>
        <point name="p6" on="ln_af" by="x2" which="bottom">0.98</point>

        <point name="p7" on="ln_af" by="x2" which="top">0.2</point>
        <point name="p8" on="ln_af" by="x2" which="bottom">0.2</point>

        <point name="p9" on="ln_af" by="x2" which="top">0.1</point>
        <point name="p10" on="ln_af" by="x2" which="bottom">0.1</point>

        <point name="pnsmc">0.952727348 0</point>

        <point name="pfle1">0.941727348 0</point>
        <point name="pfle2">0.7416642415 0</point>
        <point name="pfte1">0.3750776842 0</point>
        <point name="pfte2">0.11 0</point>

        <!-- Main spar -->
        <!--
        <line name="line_spar_top">
            <points>p1:p3</points>
        </line>
        <line name="line_spar_bottom">
            <points>p4:p2</points>
        </line>
        <line name="line_spar_web_front" type="arc">
            <start>p2</start>
            <end>p1</end>
            <curvature>0</curvature>
            <side>right</side>
            <direction>ccw</direction>
        </line>
        <line name="line_spar_web_back" type="arc">
            <start>p3</start>
            <end>p4</end>
            <curvature>0</curvature>
            <side>right</side>
            <direction>ccw</direction>
        </line>

        <line name="line_spar" method="join">
            <line>line_spar_top</line>
            <line>line_spar_bottom</line>
            <line>line_spar_web_front</line>
            <line>line_spar_web_back</line>
        </line>
        -->

        <!-- Front part and leading edge -->
        <baseline name="bsl_le_top">
            <points>p5:p3</points>
        </baseline>
        <baseline name="bsl_le">
            <points>p6:p5</points>
        </baseline>
        <baseline name="bsl_le_bottom">
            <points>p4:p6</points>
        </baseline>

        <!-- Back part and trailing edge -->
        <baseline name="bsl_te_top">
            <points>p3:p7</points>
        </baseline>
        <baseline name="bsl_te">
            <points>p7:p8</points>
        </baseline>
        <baseline name="bsl_te_bottom">
            <points>p8:p4</points>
        </baseline>

        <!-- Non-structural mass -->
        <baseline name="bsl_nsm" type="circle">
            <center>pnsmc</center>
            <radius>0.001</radius>
            <discrete by="angle">9</discrete>
        </baseline>

        <!-- Filling part -->
        
        <point name="ptfb" constraint="middle">p9 p10</point>
        <!-- <point name="ptfb">0.1 0</point> -->
        <point name="ptf">0.09 0</point>
        <line name="bsl_te_fill_bound">
            <points>p10,ptfb,p9</points>
            <!-- <point>ptfb</point>
            <angle>90</angle> -->
        </line>
    </baselines>



    <layups>
        <layup name="lyp_le_cap">
            <layer lamina="Aluminum 8009_0.01">0:1</layer>
        </layup>
        <layup name="lyp_skin">
            <layer lamina="AS4 12k/E7K8_0.0054">0:1</layer>
        </layup>
        <!--
        <layup name="lyp_spar">
            <layer lamina="AS4 12k/E7K8_0.0054">37:2</layer>
            <layer lamina="AS4 12k/E7K8_0.0054">-33:6</layer>
            <layer lamina="AS4 12k/E7K8_0.0054">-14:15</layer>
            <layer lamina="AS4 12k/E7K8_0.0054">-71:17</layer>
        </layup>
        -->
        <layup name="lyp_le">
            <layer lamina="AS4 12k/E7K8_0.0054">-77:4</layer>
        </layup>

        <layup name="lyp_te">
            <layer lamina="AS4 12k/E7K8_0.0054">-3:1</layer>
        </layup>
        <layup name="lyp_fb">
            <layer lamina="AS4 12k/E7K8">0:1</layer>
        </layup>

    </layups>

    <!--
    <component name="spar">
        <segments>
            <baseline>line_spar</baseline>
            <layup>lyp_spar</layup>
        </segments>
    </component>
    -->

    <component name="le"> <!-- depend="spar"> -->
        <segments>
            <baseline>bsl_le</baseline>
            <layup>lyp_le</layup>
        </segments>
        <segments>
            <baseline>bsl_le_top</baseline>
            <layup>lyp_le</layup>
        </segments>
        <segments>
            <baseline>bsl_le_bottom</baseline>
            <layup>lyp_le</layup>
        </segments>  
        <segments>
            <baseline>bsl_te_top</baseline>
            <layup>lyp_te</layup>
        </segments>
        <segments>
            <baseline>bsl_te</baseline>
            <layup>lyp_te</layup>
        </segments>
        <segments>
            <baseline>bsl_te_bottom</baseline>
            <layup>lyp_te</layup>
        </segments>
    </component>

    <component name="web" depend="le">
        <segment>
            <baseline>bsl_te_fill_bound</baseline>
            <layup>lyp_fb</layup>
        </segment>
    </component>

    <component name="ns_mass" type="fill">
        <baseline>bsl_nsm</baseline>
        <location>pnsmc</location>
        <material>lead</material>
        <mesh_size at="pnsmc">0.04</mesh_size>
    </component>

    <component name="fill_te" type="fill" depend="le,web">
        <!-- <baseline fillside="left">bsl_te_fill_bound</baseline> -->
        <!-- <baseline>bsl_te_fill_bound</baseline> -->
        <location>ptf</location>
        <material>AS4 12k/E7K8</material>
    </component>

    <component name="fill_front" type="fill" depend="fill_te,ns_mass">
        <location>pfle1</location>
        <material>Rohacell 70</material>
        <mesh_size at="pfle1,pfte2">0.04</mesh_size>
    </component>

    <!--
    <component name="fill_back" type="fill" depend="spar,te">
        <baseline fillside="right">bsl_te_fill_bound</baseline>
        <material>Plascore PN2-3/16OX3.0</material>
        <mesh_size at="pfte1,pfte2">0.04</mesh_size>
    </component>
    -->

    <global measure="stress">
        <include format="csv" header="1">loadcases.csv</include>
    </global>

</cross_section>
